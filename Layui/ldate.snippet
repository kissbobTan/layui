<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.1.0">
		<Header>
			<Title>日期范围组</Title>
			<Author>BobTan</Author>
			<Shortcut>l_date</Shortcut>
			<Description>Layui 日期范围组</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>beginTime</ID>
					<ToolTip>起始时间Id</ToolTip>
					<Default>起始时间Id</Default>
				</Literal>
				<Literal>
					<ID>endTime</ID>
					<ToolTip>截止时间Id</ToolTip>
					<Default>截止时间Id</Default>
				</Literal>
				<Literal>
					<ID>htmlname</ID>
					<ToolTip>HTML代码</ToolTip>
					<Default>HTML代码</Default>
				</Literal>
			</Declarations>
			<Code Language="JavaScript">
				<![CDATA[
	var laydate = layui.laydate;

	laydate.render({
		elem: '#$beginTime$'
	});
	laydate.render({
		elem: '#$endTime$'
	});
	

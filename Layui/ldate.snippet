          //    <div class="layui-input-inline" style="width:100px">
          //        <input type="text" class="layui-input" readonly="readonly" id="$endTime$" placeholder="yyyy-MM-dd">
          //    </div>
          //</div>
	$end$
    ]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	
</CodeSnippets>
